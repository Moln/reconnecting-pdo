{
    "name": "moln/reconnecting-pdo",
    "description": "Reconnecting pdo when mysql was gone away.",
    "license": "MIT",
    "keywords": [
        "pdo",
        "reconnect",
        "retry",
        "database"
    ],
    "authors": [
        {
            "name": "Moln Xie",
            "email": "moln.xie@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-pdo": "*",
        "psr/log": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Moln\\ReconnectingPDO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moln\\ReconnectingPDO\\Test\\": "test/"
        }
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "ext-xml": "*",
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
